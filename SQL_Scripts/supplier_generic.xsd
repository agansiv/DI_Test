<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.cancerresearchuk.org/di/r17/supplier/generic" xmlns:com="http://www.cancerresearchuk.org/di/r17/commonTypes" targetNamespace="http://www.cancerresearchuk.org/di/r17/supplier/generic" elementFormDefault="qualified">
	<import namespace="http://www.cancerresearchuk.org/di/r17/commonTypes" schemaLocation="commonTypesDI.xsd"/>
	<complexType name="Supporter">
		<sequence>
			<element name="dateOfBirth" type="date" minOccurs="0"/>
			<element name="primaryExternalReferenceId" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="100"/>
					</restriction>
				</simpleType>
			</element>
			<element name="secondaryExternalReferenceId" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="100"/>
					</restriction>
				</simpleType>
			</element>
			<element name="primaryDataSourceCode" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="secondaryDataSourceCode" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="forename" nillable="false">
				<simpleType>
					<restriction base="com:PartyNamePerson">
						<minLength value="1"/>
						<maxLength value="50"/>
					</restriction>
				</simpleType>
			</element>
			<element name="gender" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="initial" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="30"/>
						<pattern value="[a-zA-Z ]*"/>
					</restriction>
				</simpleType>
			</element>
			<element name="startDate" type="dateTime" nillable="false"/>
			<element name="source" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="100"/>
					</restriction>
				</simpleType>
			</element>
			<element name="statusCode" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="statusDate" type="date" nillable="false"/>
			<element name="suffix" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="15"/>
					</restriction>
				</simpleType>
			</element>
			<element name="supporterURN" minOccurs="0">
				<simpleType>
					<restriction base="com:AlphaNumeric">
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="surname" nillable="false">
				<simpleType>
					<restriction base="com:PartyNamePerson">
						<minLength value="2"/>
						<maxLength value="50"/>
					</restriction>
				</simpleType>
			</element>
			<element name="title" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<minLength value="2"/>
						<maxLength value="50"/>
						<pattern value="[a-zA-Z ]*"/>
					</restriction>
				</simpleType>
			</element>
			<element name="changeofNameIndicator" type="com:YNFlag" minOccurs="0"/>
			<element name="nonTaxPayerFlag" type="com:YNFlag" minOccurs="0"/>
			<element name="nonTaxPayerStartDate" type="date" minOccurs="0"/>
			<element name="dateOfDeath" type="date" minOccurs="0"/>
			<element name="deathNotificationDate" type="date" minOccurs="0"/>
			<element name="maritalStatus" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="address" type="tns:Address" nillable="false"/>
			<element name="secondaryAddresses" minOccurs="0">
				<complexType>
					<sequence>
						<element name="address" type="tns:Address" minOccurs="0" maxOccurs="unbounded"/>
					</sequence>
				</complexType>
			</element>
			<element name="contactInfo-Phone" type="tns:ContactInfo-Phone" minOccurs="0"/>
			<element name="contactInfo-Email" type="tns:ContactInfo-Email" minOccurs="0"/>
			<element name="contactInfo-Mobile" type="tns:ContactInfo-Mobile" minOccurs="0"/>
			<element name="giftAidDeclaration" type="tns:GiftAidDeclaration" minOccurs="0"/>
			<element name="directDebit" type="tns:DirectDebit" minOccurs="0"/>
			<element name="listOfDonations" minOccurs="0">
				<complexType>
					<sequence>
						<element name="donations" type="tns:Donations" minOccurs="0" maxOccurs="unbounded"/>
					</sequence>
				</complexType>
			</element>
			<element name="listOfSuppressionsPreferences" minOccurs="0">
				<complexType>
					<sequence>
						<element name="suppressionsPreferences" type="tns:SuppressionsPreferences" minOccurs="0" maxOccurs="unbounded"/>
					</sequence>
				</complexType>
			</element>
			<element name="listOfCampaignMembership" minOccurs="0">
				<complexType>
					<sequence>
						<element name="campaignMembership" type="tns:CampaignMembership" minOccurs="0" maxOccurs="unbounded"/>
					</sequence>
				</complexType>
			</element>
			<element name="listOfMailingActivities" minOccurs="0">
				<complexType>
					<sequence>
						<element name="mailingActivities" type="tns:MailingActivities" minOccurs="0" maxOccurs="unbounded"/>
					</sequence>
				</complexType>
			</element>
			<element name="listOfEventRegistration" minOccurs="0">
				<complexType>
					<sequence>
						<element name="eventRegistration" type="tns:EventRegistration" minOccurs="0" maxOccurs="unbounded"/>
					</sequence>
				</complexType>
			</element>
			<element name="standingOrders" type="tns:StandingOrders" minOccurs="0"/>
			<element name="listOfProfilingInfo" minOccurs="0">
				<complexType>
					<sequence>
						<element name="profilingInfo" type="tns:ProfilingInfo" minOccurs="0" maxOccurs="unbounded"/>
					</sequence>
				</complexType>
			</element>
			<element name="listOfOpportunity" minOccurs="0">
				<complexType>
					<sequence>
						<element name="opportunity" type="tns:Opportunity" minOccurs="0" maxOccurs="unbounded"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="Batch">
		<sequence>
			<element name="id" type="integer" minOccurs="0"/>
			<element name="listOfTransaction" minOccurs="0">
				<complexType>
					<sequence>
						<element name="transaction" type="tns:Transaction" minOccurs="0" maxOccurs="unbounded"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="Transaction">
		<sequence>
			<element name="reference" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="100"/>
					</restriction>
				</simpleType>
			</element>
			<element name="supporter" type="tns:Supporter" nillable="false"/>
		</sequence>
	</complexType>
	<complexType name="Address">
		<sequence>
			<element name="addressLine1" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="2"/>
						<maxLength value="200"/>
						<pattern value="[a-zA-Z0-9\-/\.&apos;&amp; ]*"/>
					</restriction>
				</simpleType>
			</element>
			<element name="addressLine2" minOccurs="0">
				<simpleType>
					<restriction base="com:AlphaNumericWithSeperators">
						<maxLength value="100"/>
					</restriction>
				</simpleType>
			</element>
			<element name="addressLine3" minOccurs="0">
				<simpleType>
					<restriction base="com:AlphaNumericWithSeperators">
						<maxLength value="100"/>
					</restriction>
				</simpleType>
			</element>
			<element name="addressLine4" minOccurs="0">
				<simpleType>
					<restriction base="com:AlphaNumericWithSeperators">
						<maxLength value="100"/>
					</restriction>
				</simpleType>
			</element>
			<element name="cherishedAddressHouseName" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="100"/>
						<pattern value="[a-zA-Z0-9\-/\.&apos;, ]*"/>
					</restriction>
				</simpleType>
			</element>
			<element name="city" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="50"/>
						<pattern value="[a-zA-Z\-/\.&apos; ]*"/>
					</restriction>
				</simpleType>
			</element>
			<element name="country" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="30"/>
						<pattern value="[a-zA-Z\- ]*"/>
					</restriction>
				</simpleType>
			</element>
			<element name="county" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="50"/>
						<pattern value="[a-zA-Z\- ]*"/>
					</restriction>
				</simpleType>
			</element>
			<element name="postalCode" minOccurs="0">
				<annotation>
					<documentation>Drives postcode anywhere lookup.  Can be freetext</documentation>
				</annotation>
				<simpleType>
					<restriction base="token">
						<pattern value="[a-zA-Z0-9 ]*"/>
					</restriction>
				</simpleType>
			</element>
			<element name="startDate" type="date" nillable="false"/>
			<element name="validationStatus" nillable="false">
				<simpleType>
					<restriction base="com:AlphaNumeric">
						<minLength value="1"/>
						<maxLength value="20"/>
					</restriction>
				</simpleType>
			</element>
			<element name="newAddressIndicator" type="com:YNFlag" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ContactInfo-Phone">
		<sequence>
			<element name="phoneNumber" minOccurs="0">
				<simpleType>
					<restriction base="com:Landline">
						<maxLength value="16"/>
					</restriction>
				</simpleType>
			</element>
		</sequence>
	</complexType>
	<complexType name="ContactInfo-Email">
		<sequence>
			<element name="emailAddress" minOccurs="0">
				<simpleType>
					<restriction base="com:EmailAddress">
						<maxLength value="100"/>
					</restriction>
				</simpleType>
			</element>
		</sequence>
	</complexType>
	<complexType name="ContactInfo-Mobile">
		<sequence>
			<element name="mobileNumber" minOccurs="0">
				<simpleType>
					<restriction base="com:Mobile">
						<maxLength value="100"/>
					</restriction>
				</simpleType>
			</element>
		</sequence>
	</complexType>
	<complexType name="Donations">
		<sequence>
			<element name="amount" nillable="false">
				<simpleType>
					<restriction base="decimal">
						<minExclusive value="0"/>
						<fractionDigits value="2"/>
						<totalDigits value="9"/>
						<maxExclusive value="10000000"/>
					</restriction>
				</simpleType>
			</element>
			<element name="bankAccountCode-CRUK" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="50"/>
					</restriction>
				</simpleType>
			</element>
			<element name="dataSource" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="dateReceived" type="date" nillable="false"/>
			<element name="donationType" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="eventCode" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="financialPaymentReference" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="30"/>
						<pattern value="[a-zA-Z0-9/-]*"/>
					</restriction>
				</simpleType>
			</element>
			<element name="letterCode" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="resCode" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="50"/>
					</restriction>
				</simpleType>
			</element>
			<element name="paymentMethod" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="paymentStatus" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="personalGiftAid" nillable="false">
				<simpleType>
					<restriction base="com:YNFlag"/>
				</simpleType>
			</element>
			<element name="reasonNotGiftAid" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="product" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="100"/>
					</restriction>
				</simpleType>
			</element>
			<element name="source" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="webPageId" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="255"/>
						<pattern value="[a-zA-Z0-9\-]*"/>
					</restriction>
				</simpleType>
			</element>
			<element name="originatorPaymentID" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="toBeGiftAided" nillable="false">
				<simpleType>
					<restriction base="com:YNFlag"/>
				</simpleType>
			</element>
			<element name="letterCodeGAD" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="methodGAD" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="motivation" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="100"/>
					</restriction>
				</simpleType>
			</element>
			<element name="inMemoryName" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="100"/>
					</restriction>
				</simpleType>
			</element>
			<element name="celebrantDataSource" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="celebrantExternalReference" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="100"/>
					</restriction>
				</simpleType>
			</element>
			<element name="paymentProviderTransactionId" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="gadType" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="excludefromAgresso" type="com:YNFlag" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="GiftAidDeclaration">
		<sequence>
			<element name="confirmedOn" type="date" minOccurs="0"/>
			<element name="declarationDate" type="date" nillable="false"/>
			<element name="letterCode" minOccurs="0">
				<simpleType>
					<restriction base="token">
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="method" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="reference" minOccurs="0">
				<simpleType>
					<restriction base="com:AlphaNumeric">
						<maxLength value="100"/>
					</restriction>
				</simpleType>
			</element>
			<element name="source" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="startDate" type="date" nillable="false"/>
			<element name="gadType" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
		</sequence>
	</complexType>
	<complexType name="DirectDebit">
		<sequence>
			<element name="accountName" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="2"/>
						<maxLength value="50"/>
					</restriction>
				</simpleType>
			</element>
			<element name="amount" nillable="false">
				<simpleType>
					<restriction base="decimal">
						<minExclusive value="0"/>
						<fractionDigits value="2"/>
						<totalDigits value="9"/>
						<maxExclusive value="10000000"/>
					</restriction>
				</simpleType>
			</element>
			<element name="bankAccountNumber" nillable="false">
				<simpleType>
					<restriction base="com:NumericOnly">
						<minLength value="8"/>
						<maxLength value="10"/>
					</restriction>
				</simpleType>
			</element>
			<element name="bankSortCode" nillable="false">
				<simpleType>
					<restriction base="com:NumericOnly">
						<length value="6"/>
					</restriction>
				</simpleType>
			</element>
			<element name="bankAccountCode-CRUK" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="50"/>
					</restriction>
				</simpleType>
			</element>
			<element name="frequency" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="product" nillable="false">
				<simpleType>
					<restriction base="token">
						<minLength value="1"/>
						<maxLength value="100"/>
					</restriction>
				</simpleType>
			</element>
			<element name="reference" nillable="false">
				<simpleType>
					<restriction base="com:AlphaNumeric">
						<minLength value="1"/>
						<maxLength value="30"/>
					</restriction>
				</simpleType>
